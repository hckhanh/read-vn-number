{
  "name": "read-vn-number",
  "version": "3.2.1",
  "description": "Read Vietnamese number like a Vietnamese",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/es6/index.js",
  "es2015": "dist/es6/index.js",
  "esnext": {
    "main": "dist/esnext/index.js"
  },
  "exports": {
    ".": {
      "browser": "./dist/amd/index.js",
      "import": "./dist/es6/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hckhanh/read-vn-number.git"
  },
  "keywords": [
    "reader",
    "read",
    "number",
    "vn",
    "vietnam",
    "vietnamese",
    "vietnamese-language",
    "vietnamese-currency",
    "currency",
    "vnd",
    "vn-lang",
    "lang",
    "language",
    "languages",
    "language-processing",
    "text-generation",
    "helper",
    "helpers",
    "helpers-library",
    "helper-tool",
    "helper-methods",
    "helper-class",
    "helper-functions",
    "helper-script"
  ],
  "author": "Khanh Hoang",
  "license": "MIT",
  "scripts": {
    "build:cjs": "tsc",
    "build:es6": "tsc --build tsconfig.es6.json",
    "build:esnext": "tsc --build tsconfig.esnext.json",
    "build:amd": "tsc --build tsconfig.amd.json",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "test:deps": "depcruise --config .dependency-cruiser.js src",
    "test:prettier": "prettier --check .",
    "test:eslint": "eslint src --ext .ts --max-warnings 10",
    "test:skypack": "package-check",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@skypack/package-check": "^0.2.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "dependency-cruiser": "^9.11.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "mocha": "^8.1.2",
    "prettier": "2.2.0",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
