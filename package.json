{
  "name": "read-vn-number",
  "version": "2.0.1",
  "description": "Read Vietnamese number like a Vietnamese",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/es6/index.js",
  "es2015": "dist/es6/index.js",
  "esnext": {
    "main": "dist/esnext/index.js"
  },
  "scripts": {
    "build:cjs": "tsc",
    "build:es6": "tsc --build tsconfig.es6.json",
    "build:esnext": "tsc --build tsconfig.esnext.json",
    "build:amd": "tsc --build tsconfig.amd.json",
    "pretest": "eslint src --ext .ts",
    "test": "mocha -r ts-node/register tests/**/*.ts",
