{
  "name": "read-vn-number",
  "version": "3.2.1",
  "description": "Read Vietnamese number like a Vietnamese",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./es/index.js",
    "require": "./cjs/index.js"
  },
  "files": [
    "cjs",
    "es",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hckhanh/read-vn-number.git"
  },
  "keywords": [
    "reader",
    "read",
    "number",
    "vn",
    "vietnam",
    "vietnamese",
    "vietnamese-language",
    "vietnamese-currency",
    "currency",
    "vnd",
    "vn-lang",
    "lang",
    "language",
    "languages",
    "language-processing",
    "text-generation",
    "helper",
    "helpers",
    "helpers-library",
    "helper-tool",
    "helper-methods",
    "helper-class",
    "helper-functions",
    "helper-script"
  ],
  "author": "Khanh Hoang",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "build:cjs": "tsc --outDir cjs",
    "build:es": "tsc --module esnext --outDir es",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir types",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "test:deps": "depcruise --config .dependency-cruiser.js src",
    "test:prettier": "prettier --check .",
    "test:eslint": "eslint src --ext .ts --max-warnings 10",
    "test:skypack": "package-check",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@skypack/package-check": "^0.2.2",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "chai": "^4.3.4",
    "dependency-cruiser": "^10.4.0",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "mocha": "^8.3.2",
    "prettier": "2.2.1",
    "semantic-release": "^17.4.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
